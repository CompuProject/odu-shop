/* end 4. TYPOGRAPHY */
/* begin 5.FORMS */
.input, .textarea {
  display: inline-block;
  border-radius: 5px;
  border: solid 1px #e8e8e8;
  height: 42px;
  line-height: 40px;
  padding: 0 18px;
