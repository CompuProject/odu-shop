  width: 170px;
  height: 100%;
  letter-spacing: 2px;
  text-transform: uppercase;
  border-radius: 40px;
  outline: none; }

.checkbox {
  text-align: left;
  margin-bottom: 10px; }
  .checkbox input {
    display: none; }
  .checkbox label {
    position: relative;
    padding-left: 25px;
    font-size: 14px;
    cursor: pointer;
    color: #242424;
    line-height: 20px; }
  .checkbox label:before, .checkbox label:after {
    font-family: FontAwesome;
    font-size: 20px;
    /*absolutely positioned*/
    position: absolute;
    top: -1px;
    left: 0;
    right: 0; }
  .checkbox label:before {
    content: '\f096';
    /*checkbox unchecked */ }
  .checkbox label:after {
    content: '\f00c';
    /*checkbox checked*/
    width: 0;
    overflow: hidden;
    opacity: 0.5;
    font-size: 14px;
    top: -2px;
    left: 1px;
    color: #e2574c;
    transition: all 0.50s; }
  .checkbox input:checked + label:after {
    width: 16px;
    opacity: 1;
    margin-right: 90px; }

.radio {
  margin-bottom: 10px;
  text-align: left; }
  .radio input {
    display: none; }
  .radio label:before, .radio label:after {
    font-family: FontAwesome;
    font-size: 20px; }
  .radio label {
    position: relative;
    padding-left: 25px;
    cursor: pointer; }
    .radio label:before {
      content: "\f10c";
      position: absolute;
      left: 0;
      top: 0; }
    .radio label:after {
      content: "\f111";
      width: 0px;
      position: absolute;
      left: 5px;
      top: 6px;
      color: #e2574c;
      font-size: 8px;
      transition: all 0.50s;
      overflow: hidden;
      opacity: 0.5; }
  .radio input:checked + label:after {
    width: 16px;
    opacity: 1; }

.textarea {
  width: 100%;
  min-height: 100px;
  resize: vertical; }

